  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@react-native/metro-config": "^0.72.6",
    "@types/react": "^18.2.14",
    "@types/react-native": "~0.72.2",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "prettier": "^2.4.1",
    "react-native-svg-transformer": "^1.0.0",
    "typescript": "4.8.4"
